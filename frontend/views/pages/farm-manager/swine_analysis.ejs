<%- include("../../partials/farm-manager/header") %>
<%- include("../../partials/farm-manager/navbar") %>
<%- include("../../partials/farm-manager/sidepanel") %>

   <!-- Main content -->
    <main class="main-content">
      <div class="container-fluid">
        <!-- Dashboard Header card -->
        <div class="card page-card">
          <div class="card-body">
            <h4 class="page-title">BREED QUALITY ANALYSIS</h4>
                    <!-- Rankings -->
                    <!-- <div class="card"> -->
                        <h3>üèÜ Breed Quality Rankings</h3>
                        <p class="muted">Ranked based on performance, physical traits, and reproductive history.</p>

                        <table>
                            <thead>
                            <tr>
                                <th>Rank</th>
                                <th>Swine ID</th>
                                <th>Sex</th>
                                <th>Breed</th>
                                <th>Quality Index</th>
                            </tr>
                            </thead>
                            <tbody id="rankingTable">
                            <tr>
                                <td colspan="5" style="text-align:center;">Loading analytics data‚Ä¶</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                    </div>

                    <!-- Compatibility -->
                    <div class="card">
                                  <div class="card-body">

                        <h3>üß¨ Breeding Compatibility Checker</h3>
                        <p class="muted">Analyze genetic compatibility between a selected pair to minimize inbreeding risks.</p>

                        <div class="pair-select">
                            <div>
                                <label>Sow (Female)</label>
                                <select id="femaleSelect">
                                    <option value="">-- Select Sow --</option>
                                </select>
                            </div>

                            <div class="pair-symbol">√ó</div>

                            <div>
                                <label>Boar (Male)</label>
                                <select id="maleSelect">
                                    <option value="">-- Select Boar --</option>
                                </select>
                            </div>
                        </div>

                        <button id="calcBtn" onclick="calculateMatch()" class="btn-submit">
                            Run Compatibility Analysis
                        </button>

                        <div id="matchResult" class="match-box">
                            <small class="muted" style="text-transform: uppercase;">Estimated Match Score</small>
                            <div id="matchScore">0%</div>
                            <div id="matchLogs"></div>
                        </div>
                    </div>
</div>  
          </div>
        </div>
      </div>
    </main>
